{
  "version": 2,
  "builds": [
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "**/*.json", "use": "@vercel/static" },
    { "src": "**/*.ts", "use": "@vercel/static" },
    { "src": "**/*.tsx", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}